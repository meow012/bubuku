    "buku_features": "restart_on_exhibitor,graceful_terminate,use_ip_address",
    "jmx_port": 8004,
    "kafka_heap_opts": "-Xmx6G -Xms6G",
    "startup_timeout_type": "linear",
    "startup_timeout_initial": 300,
    "startup_timeout_step": 60
